{{#if user}}
<div class="jumbotron jumb-home">
  <div class="col-md-3">
    <img alt="Image" class="img-responsive" src="/assets/enseja_logo_split.png">
  </div>
  <div class="col-md-9"></div>
  <h1>ENSEJA</h1>

  <h4>Welcome {{ user.displayName }}!</h4>
</div>
<br>

<div>
  <div class="card-geral col-md-4">
    <div class="card bg-dark text-white">
      <img class="card-img icone-dash" src="/assets/foguete.png" alt="Card image">
      <div class="card-img-overlay">
        <h5 class="card-title">Skills</h5>
        <p class="card-text">This is a wider card with supporting text below as a natural lead-in to additional content.</p>
      </div>
    </div>
  </div>
  <div class="card-geral col-md-4">

    <div class="card bg-dark text-white">
      <img class="card-img icone-dash" src="/assets/training.png" alt="Card image">
      <div class="card-img-overlay">
        <h5 class="card-title">Trainings</h5>
        <p class="card-text">This is a wider card with supporting text below as a natural lead-in to additional content.</p>
      </div>
    </div>
  </div>
  <div class="card-geral col-md-4">

    <div class="card bg-dark text-white ">
      <img class="card-img icone-dash" src="/assets/medal.png" alt="Card image">
      <div class="card-img-overlay">
        <h5 class="card-title">Certificates</h5>
        <p class="card-text">This is a wider card with supporting text below as a natural lead-in to additional content.</p>
      </div>
    </div>
  </div>
</div>


{{else}}
<script> window.location ='/';</script>
{{/if}}