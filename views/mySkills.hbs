{{#if user}}
<br>

<h1> My Skills </h1>
<br>

<div class="panel panel-primary">
    <div class="panel-heading collapsed" data-toggle="collapse" data-parent="#accordion" data-target="#collapse1">
      <h4 class="panel-title">
        F5
      </h4>
      <div class="panel-subtitle">Os profissionais com essa tecnologia você encontra aqui</div>
    </div>
    <div id="collapse1" class="panel-collapse collapse">
      <div class="panel-body">
        <div class="table-responsive scrollbars">
          <table class="table table-hover">
            <thead>
              <tr>
                <th>Name</th>
                <th>Email</th>
                <th>Job</th>
                <th>Office</th>
                <th>LTM</th>
                <th>GTM</th>
                <th>ASM</th>
                <th>APM</th>
                <th>BIG IQ</th>
                <th>Actions</th>
              </tr>
            </thead>
            <tbody>
              <!-- fazer for each aqui-->
              {{#compare user.email  data.email operator="==="}}
              <tr>                
                <td>{{data.name}}</td>
                <td>{{data.email}}</td>
                <td>{{this.job}}</td>
                <td>{{this.office}}</td>
                <td>{{this.LTM}}</td>
                <td>{{this.GTM}}</td>
                <td>{{this.ASM}}</td>
                <td>{{this.APM}}</td>
                <td>{{this.BIGIQ}}</td>
                <td><i class="fa fa-edit" aria-hidden="true"></i><i class="fa fa-trash" aria-hidden="true"></i></td>
              </tr>
              {{/compare}}
              <!---->
            </tbody>
            <tfoot>
              <tr>
                <td colspan="6">
                  <nav aria-label="Page navigation">
                    <ul class="pagination pull-right no-margin">
                      <li class="pagination-extra">
                        <label for="rowsPerPage">Rows per page</label>
                        <select id="rowsPerPage" class="form-control input-sm">
                          <option>5</option>
                          <option selected>10</option>
                          <option>20</option>
                          <option>50</option>
                        </select>
                      </li>
                      <li class="pagination-extra">1-10 of 100</li>
                      <li>
                        <a>
                          <span aria-label="Previous" class="fa fa-angle-left"></span>
                        </a>
                      </li>
                      <li>
                        <a>
                          <span aria-label="Next" class="fa fa-angle-right"></span>
                        </a>
                      </li>
                    </ul>
                  </nav>
                </td>
              </tr>
            </tfoot>
          </table>
        </div>
      </div>
    </div>
  </div>


